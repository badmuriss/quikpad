    "mongodb": "^6.16.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "jest": "^30.0.5",
    "mongodb-memory-server": "^10.1.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
